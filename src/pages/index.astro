---
import Layout from "../layouts/Layout.astro";
import data from "../util/data";
import Banner from "../components/Banner.astro";
import Card from "../components/Card.astro";
import SaveTheDay from "../components/SaveTheDay.astro";
import Itinerario from "../components/Itinerario.astro";
import Indicaciones from "../components/Indicaciones.astro";
---

<Layout title={data.nombreNovia + " & " + data.nombreNovio + " Invitacion"} >
  <Banner
		data={data}
	/>
  <Card/>
  <SaveTheDay
    data={data}
  />
  <Itinerario
    data={data}
  />
  <Indicaciones
    data={data}
  />
</Layout>



